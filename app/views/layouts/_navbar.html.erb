<nav>
    <ul>
        <% if !user_signed_in? %>
            <li><%= link_to 'Signup', new_user_registration_path %></li>
            <li><%= link_to 'Login', login_path %></li>
        <% else %>
            <li><%= link_to "#{current_user.name}'s Homepage", destinations_path %></li>
            <li><%= link_to 'Logout', destroy_user_session_path, method: 'delete' %></li>
        <% end %>
    </ul>
</nav>